{\n  name: 'AtPhoneSelect',\n  props: {\n    ...InputElements.props,\n    id: { type: String, default: () => generatorId('at-phone-select-') },\n    modelValue: { type: [String, Number], default: '' },\n    label: { type: String, default: 'Номер телефона' },\n    placeholder: { type: String, default: '99 999 99 99' },\n    mask: { type: String, default: '## ### ## ##' },\n  },\n  setup() {\n    let country = ref('998');\n    let phone = ref('');\n    const prefix = [\n      { value: '998', title: '🇺🇿 (+998)' },\n      { value: '992', title: '🇹🇯 (+992)' },\n    ];\n\n    return { country, phone, prefix };\n  },\n  render() {\n    return h(\n      AtInput,\n      {\n        id: this.id,\n        addOnBeforeSelect: true,\n        modelValue: this.phone,\n        mask: this.mask,\n        placeholder: this.placeholder,\n        label: this.label,\n        error: this.error,\n        success: this.success,\n        disabled: this.disabled,\n        'onUpdate:modelValue': value => {\n          this.phone = value;\n          this.$emit('update:modelValue', this.country + (/\\s/g.test(value) ? value.replaceAll(' ', '') : value));\n          this.$emit('onChange', value);\n        },\n      },\n      {\n        addOnBefore: () =>\n          h(AtInputAddOnSelect, {\n            items: this.prefix,\n            modelValue: this.country,\n            beforeInput: true,\n            'onUpdate:modelValue': value => {\n              this.$emit(\n                'update:modelValue',\n                value + (/\\s/g.test(this.phone) ? this.phone.replaceAll(' ', '') : this.phone)\n              );\n              this.country = value;\n            },\n          }),\n      }\n    );\n  },\n});\n"]}]}